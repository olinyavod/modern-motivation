
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/Site.cshtml";
}

<h2>Достижения сотрудника @ViewBag.Name</h2>

<table class="table">
    <div class="row">
        @foreach (Motivation.Models.UserAchivment item in @ViewBag.Achivements)
        {
            <div class="col-md-1"><img src="@item.AchivmentType.ImageUrl" style="width:200px" /></div>
        }
    </div>
</table>    
