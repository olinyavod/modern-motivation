@model IEnumerable<Motivation.Models.Compitition>

@{
    ViewBag.Title = "GetCompetitions";
    Layout = "~/Views/Shared/Site.cshtml";
}

@section RenderHeader
{
<script>
    $(document).ready(function () {
        $('#clock').countdown('2020/10/10', function (event) {
            var $this = $(this).html(event.strftime(''
              + '<span>%w</span> weeks '
              + '<span>%d</span> days '
              + '<span>%H</span> hr '
              + '<span>%M</span> min '
              + '<span>%S</span> sec'));
        });
    });
</script>
}
<div id="clock"></div>

<h2>GetCompetitions</h2>
@foreach (Motivation.Models.Compitition comp in Model)
{
    @comp.Comment
    <div id="clock"></div>


}

